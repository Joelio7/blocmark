<h3>New Bookmark<h3>
  <div class="col-md-8">
<%= form_for [topic, bookmark] do |f| %>
  <div class="new-bookmark">
  <%= f.text_field :url, class: 'form-control', placeholder: "Enter Bookmark" %>
</div>
  <div class="form-group">
  <%= f.submit "save bookmark", class:  'btn btn-success' %>
</div>
<% end %>
</div>
