<h1>Bookmarks#edit</h1>
<div class="col-md-8">
    <%= form_for @bookmark do |f| %>
      <div class="form-group">
        <%= f.label :url %>
        <%= f.text_field :url, class: 'form-control', placeholder: "update bookmark" %>
      </div>
      <div class="form-group">
         <%= f.submit "Save", class: 'btn btn-success' %>
       </div>
     <% end %>
